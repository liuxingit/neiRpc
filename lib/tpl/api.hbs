/**
 * 类名：{{item.sClassName}}
 */
import ajax from '@/common/utils/ajax';

{{#each item.paths}}
/**
    * name：{{name}}
    * desc：{{description}}
    * 接口名：/api/{{sClassName}}/{{path}}
    * 负责人：{{realname}}
    * params: {
        {{#each params}}
            {{name}}: {{#if isArray}}Array{{else}}{{typeName}}{{/if}} {{#if required}}必需{{else}}非必须{{/if}} {{description}}
        {{/each}}
    * }
    */
{{#ifExpress method 'get'}}
export function {{path}}(params) {
{{else}}
export function {{path}}(data) {
{{/ifExpress}}
    return ajax.{{#if method}}{{method}}{{else}}post{{/if}}('{{path}}', {
        {{#ifExpress method 'get'}}
        params 
        {{else}}
        data
        {{/ifExpress}} 
    });
}

{{/each}}